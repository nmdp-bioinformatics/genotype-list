<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


	<servlet>
		<servlet-name>FileOAuthServlet</servlet-name>
		<servlet-class>org.immunogenomics.gl.oauth.toy.portal.FileOAuthServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>ToyOAuthServlet</servlet-name>
		<servlet-class>org.immunogenomics.gl.oauth.toy.portal.ToyOAuthServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>FileOAuthServlet</servlet-name>
		<url-pattern>/oauth/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FileOAuthServlet</servlet-name>
		<url-pattern>/file/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ToyOAuthServlet</servlet-name>
		<url-pattern>/toy/*</url-pattern>
	</servlet-mapping>

</web-app>
