@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix gl: <http://gl.immunogenomics.org/gl-ontology/1.0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<${alleleList.Id}> a <http://gl.immunogenomics.org/gl-ontology/AlleleList> ;
  dc:title "${alleleList.Id}" ;
  dc:identifier "${alleleList.Id}" ;
  rdfs:label "${alleleList.Id}" ;
  rdfs:comment "An allele list with ${alleleList.Alleles.size()} allele#if (${alleleList.Alleles.size()} != 1)s#end represented by glstring ${alleleList.Glstring}" ;
  gl:hasGlstring "${alleleList.Glstring}" ;
#foreach ($a in ${alleleList.Alleles})
  gl:hasAllele <${a.Id}> ;
#end