<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:gl="http://immunogenomics.org/gl-ontology/1.0"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="${allele.Id}">
    <rdf:type rdf:resource="http://immunogenomics.org/gl-ontology/Allele"/>
    <dc:title>${allele.Id}</dc:title>
    <dc:identifier>${allele.Id}</dc:identifier>
    <rdfs:label>${allele.Id}</rdfs:label>
    <rdfs:comment>An allele with accession ${allele.Accession} and locus ${allele.Locus.Glstring} represented by the glstring ${allele.Glstring}</rdfs:comment>
    <gl:hasAccession>${allele.Accession}</gl:hasAccession>
    <gl:hasGlstring>${allele.Glstring}</gl:hasGlstring>
    <gl:hasLocus rdf:Resource="${allele.Locus.Id}"/>
  </rdf:Description>
</rdf:RDF>
