@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix gl: <http://immunogenomics.org/gl-ontology/1.0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<${allele.Id}> a <http://immunogenomics.org/gl-ontology/Allele> ;
  dc:title "${allele.Id}" ;
  dc:identifier "${allele.Id}" ;
  rdfs:label "${allele.Id}" ;
  rdfs:comment "An allele with accession ${allele.Accession} and locus ${allele.Locus.Glstring} represented by the glstring ${allele.Glstring}" ;
  gl:hasAccession "${allele.Accession}" ;
  gl:hasGlstring "${allele.Glstring}" ;
  gl:hasLocus <${allele.Locus.Id}> ;
