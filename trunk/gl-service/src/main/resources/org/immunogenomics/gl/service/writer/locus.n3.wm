@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix gl: <http://gl.immunogenomics.org/gl-ontology/1.0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<${locus.Id}> a <http://gl.immunogenomics.org/gl-ontology/Locus> ;
  dc:title "${locus.Id}" ;
  dc:identifier "${locus.Id}" ;
  rdfs:label "${locus.Id}" ;
  rdfs:comment "A locus represented by glstring ${locus.Glstring}" ;
  gl:hasGlstring "${locus.Glstring}" ;
