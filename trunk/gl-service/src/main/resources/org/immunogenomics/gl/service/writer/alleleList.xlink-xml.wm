<?xml version="1.0" encoding="UTF-8"?>
<glx:AlleleList xmlns:glx="https://gl.immunogenomics.org/gl-resource-xlink" xmlns:xlink="http://www.w3.org/1999/xlink">
  <glx:id>${alleleList.Id}</glx:id>
  <glx:glstring>${alleleList.Glstring}</glx:glstring>
  <glx:alleles>
#foreach ($allele in ${alleleList.Alleles})
    <glx:allele xlink:href="${allele.Id}"></glx:allele>
#end
  </glx:alleles>
</glx:AlleleList>
